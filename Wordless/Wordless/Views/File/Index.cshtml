
@using (Html.BeginForm("UploadFile", "File", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        <div>
            <input type="file" id="upload" name="upload" />
        </div>
    </div>
    <div class="form-group">
        <div>
            <input type="submit" value="Upload" class="btn btn-default" />
        </div>
    </div>
}


